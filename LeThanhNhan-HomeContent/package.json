{
  "devDependencies": {
    "@webpack-cli/generators": "^2.4.1",
    "autoprefixer": "^10.4.0",
    "css-loader": "^6.5.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "version": "1.0.0",
  "description": "Project used for making components",
  "name": "@tunanyugen/component-maker",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "setup": "composer install && npm install && npx setup",
    "make-component": "npx make-component",
    "dev": "concurrently \"php -S localhost:8000 public_html/index.php\" \"npm run watch\"",
    "update": "concurrently \"npm update @tunanyugen/component-maker-scripts\""
  },
  "dependencies": {
    "@tunanyugen/component-maker-scripts": "github:tunanyugen/component-maker-scripts",
    "concurrently": "^7.0.0",
    "dotenv": "^10.0.0",
    "uuid": "^8.3.2"
  }
}
